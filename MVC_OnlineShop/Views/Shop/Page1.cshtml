@using MVC_OnlineShop.Models
@model MVC_OnlineShop.Models.Product

@{
    ViewBag.Title = "Page1";
}

<h2>Page1</h2>

<table>
    @{
        var pList = ViewData[ "productList" ] as List<Product>;

        foreach (var p in pList) {
            <tr>                
                <td>@p.Name</td>
                <td>@p.Price</td>
            </tr>
        }
    }



</table>


